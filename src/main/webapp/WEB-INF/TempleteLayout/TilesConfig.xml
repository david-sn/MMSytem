       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

    <definition name="defaultTemplate" template="/WEB-INF/TempleteLayout/layout.jsp">
        <put-attribute name="header" value="/WEB-INF/TempleteLayout/Header.jsp" />
        <put-attribute name="body" value="/WEB-INF/TempleteLayout/body.jsp" />
        <put-attribute name="footer" value="/WEB-INF/TempleteLayout/Footer.jsp" />
        <put-attribute name="title" value="MMS" />
        <put-attribute name="currentActive" value="Home" />
    </definition>
    <definition name="body2" extends="defaultTemplate">
        <put-attribute name="body" value="/WEB-INF/JSP/body2.jsp" />
        <put-attribute name="title" value="Home" />
        <put-attribute name="currentActive" value="Home" />
    </definition>
</tiles-definitions>
